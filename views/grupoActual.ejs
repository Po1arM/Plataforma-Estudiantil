        <div class="content-body"><!-- Chart -->
      <div class="card">
        <div class="card-header">
          <% if (aux[0] === 1) {%>
            <h1>Periodo para publicación de calificaciones abierto</h1>
          </div>
          <div class="card-content" aria-expanded="true">
              <div class="card-body">
                  <div class="row">
                      <div class="col-xl-6 ">
                      <h4>Desde <%= (aux[2].getDate()+1) +'/' + (aux[2].getMonth()+1) + '/' + aux[2].getFullYear()%></h4>
                      </div>
                      <div class="col-xl-6 ">
                      <h4>Hasta <%= (aux[3].getDate()+1) +'/' + (aux[3].getMonth()+1) + '/' + aux[3].getFullYear()%></h4>
                      </div>
              </div>
              </div>
          </div>
          <%} else {%>
            <h1>Proximo periodo de publicación de calificaciones</h1>
          </div>
          <div class="card-content" aria-expanded="true">
              <div class="card-body">
                  <div class="row">
                      <div class="col-xl-6 ">
                      <h4>Empieza <%= (aux[3].getDate()+1) +'/' + (aux[3].getMonth()+1) + '/' + aux[3].getFullYear()%></h4>
                      </div>
                      
              </div>
              </div>
          </div>
            <%}%>
        
    </div>

<div class = "row">
    <div class="col-xl-4 col-lg-6 col-md-12">
        <h1>Tus grupos</h1>
    </div>
</div>

<div class="table-responsive">
    <table  class="table table-bordered bg-white">
        <thead class="thead-dark">
            <th>Codigo</th>
            <th>Curso</th>
            <th>Nivel</th>
            <th>Materia</th>
            <th>Horario</th>
            <th>Maestro</th>
        </thead>
        <tbody>
            <% for(var i = 0; i < grupos.length; i++) { %>
                <tr>
                    <td> <%= grupos[i].codigo %> </td>
                    <td> <%= grupos[i].curso %> </td>
                    <td> <%= grupos[i].nivel %> </td>
                    <td> <%= grupos[i].materia %> </td>
                    <td> <%= grupos[i].horario %> </td>
                    <td> <%= grupos[i].maestro %> </td>
                    <td><a href="/estudiantesDeGrupo/<%= grupos[i]._id%>/<%= id %>"><i class="fa fa-eye" ></i></a></td>
                
                  </tr>
            <% }%>  
        </tbody>
                    
    </table>

